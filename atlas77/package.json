{
  "name": "atlas77",
  "displayName": "atlas",
  "description": "Syntax Highlighting support for the Atlas77 language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "atlas77",
      "aliases": ["Atlas77", "atlas77"],
      "extensions": [".atlas"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "atlas77",
      "scopeName": "source.atlas77",
      "path": "./syntaxes/atlas77.tmLanguage.json"
    }]
  }
}
